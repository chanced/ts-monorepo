{
  "name": "ts-monorepo",
  "version": "0.0.1",
  "description": "typescript monorepo",
  "author": "chance dinkins",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "api": "yarn workspace @ts-monorepo/api",
    "leaf": "yarn workspace @ts-monorepo/leaf",
    "core": "yarn workspace @ts-monorepo/core",
    "build:api": "yarn api tsc --build",
    "start:api": "yarn api ts-node -r tsconfig-paths/register src/index.ts"
  },
  "dependencies": {
    "@types/node": "^14.0.27",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lerna": "^3.22.1",
    "prettier": "^2.0.5",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1"
  }
}
